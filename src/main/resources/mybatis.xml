<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.base.BaseMapper">
    <!--查询全部的-->
    <select id="loadModeForAll" parameterType="Object">
        ${sql}
    </select>
    <!--分页查询的-->
    <select id="loadModeForList" parameterType="Object">
        ${sql}
    </select>
    <!--查询分页数的-->
    <select id="loadModeForCount" parameterType="Object">
        ${sql}
    </select>
    <!--查询单个的-->
    <select id="loadModeForOne" parameterType="Object" >
        ${sql}
    </select>
    <!--插入-->
    <insert id="addModeForOne" parameterType="Object">
        ${sql}
    </insert>
    <!--删除-->
    <delete id="delModeForOne" parameterType="Object">
        ${sql}
    </delete>
    <!--修改-->
    <update id="updateModeForOne" parameterType="Object">
        ${sql}
    </update>
</mapper>